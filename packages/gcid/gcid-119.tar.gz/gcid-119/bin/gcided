#!/usr/bin/env python3
# This file is placed in the Public Domain.


""


import inspect


from gcid.handler import Command
from gcid.objects import Wd
from gcid.scanner import scan
from gcid.utility import wait


from gcid.modules import cmd, irc, mdl, rss


Wd.workdir = "/var/lib/gcid/"


def main():
    scan(cmd)
    scan(irc)
    scan(mdl)
    scan(rss)
    irc.init()
    mdl.init()
    rss.init()
    wait()


main()
