{
    "logs": {
        "level": "debug",
        "transports": {
            "console": {
                "enabled": false
            },
            "file": {
                "enabled": false,
                "name": "refinitiv-data-lib.log"
            }
        }
    },
    "sessions": {
        "default": "desktop.default",
        "platform": {
            "default": {
                "app-key": "${YOUR_APP_KEY}",
                "username": "${YOUR_REFINITIV_DATA_PLATFORM_LOGIN}",
                "password": "${YOUR_REFINITIV_DATA_PLATFORM_PASSWORD}"
            }
        },
        "desktop": {
            "default": {
                "app-key": "${YOUR_APP_KEY}"
            }
        }
    }
}