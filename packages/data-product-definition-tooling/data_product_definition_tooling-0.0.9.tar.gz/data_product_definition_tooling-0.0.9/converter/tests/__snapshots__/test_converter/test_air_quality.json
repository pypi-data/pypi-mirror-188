{
  "components": {
    "schemas": {
      "CurrentAirQualityRequest": {
        "properties": {
          "lat": {
            "description": "The latitude coordinate of the desired location",
            "example": 60.192059,
            "maximum": 90.0,
            "minimum": -90.0,
            "title": "Latitude",
            "type": "number"
          },
          "lon": {
            "description": "The longitude coordinate of the desired location",
            "example": 24.945831,
            "maximum": 180.0,
            "minimum": -180.0,
            "title": "Longitude",
            "type": "number"
          }
        },
        "required": [
          "lat",
          "lon"
        ],
        "title": "CurrentAirQualityRequest",
        "type": "object"
      },
      "CurrentAirQualityResponse": {
        "properties": {
          "airQualityIndex": {
            "description": "Current air quality index.\nRanges:\n0-50 Good;\n51-100 Moderate;\n101-150 Unhealthy For Sensitive Groups;\n151-200 Unhealthy;\n201-300 Very Unhealthy;\n301+ Hazardous",
            "example": 30,
            "minimum": 0.0,
            "title": "Air Quality Index",
            "type": "integer"
          },
          "attribution": {
            "description": "List of text to show required credits to data sources",
            "example": [
              "Moscow State environmental monitoring"
            ],
            "items": {
              "type": "string"
            },
            "title": "Source Attribution",
            "type": "array"
          },
          "timestamp": {
            "description": "Current timestamp in RFC 3339 format",
            "example": "2020-04-03T13:00:00Z",
            "title": "Timestamp",
            "type": "string"
          }
        },
        "required": [
          "airQualityIndex",
          "timestamp",
          "attribution"
        ],
        "title": "CurrentAirQualityResponse",
        "type": "object"
      },
      "HTTPValidationError": {
        "properties": {
          "detail": {
            "items": {
              "$ref": "#/components/schemas/ValidationError"
            },
            "title": "Detail",
            "type": "array"
          }
        },
        "title": "HTTPValidationError",
        "type": "object"
      },
      "ValidationError": {
        "properties": {
          "loc": {
            "items": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "integer"
                }
              ]
            },
            "title": "Location",
            "type": "array"
          },
          "msg": {
            "title": "Message",
            "type": "string"
          },
          "type": {
            "title": "Error Type",
            "type": "string"
          }
        },
        "required": [
          "loc",
          "msg",
          "type"
        ],
        "title": "ValidationError",
        "type": "object"
      }
    }
